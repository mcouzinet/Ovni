package  {		import flash.display.MovieClip;	import flash.utils.*;	import flash.events.*;		public class Mouton extends MovieClip {				public var numero:Number;		private const vitesseUp:Number = 2;		public static const CHOPPE:String = "moutonChoppe";		public var îlMonte:Boolean = false;				public function Mouton() {			y = 530 + Math.random()*50;			x = -120;		}				public function monte(){			gotoAndPlay('monte');			îlMonte = true;			addEventListener(Event.ENTER_FRAME, frameRateUp);		}				private function frameRateUp(e:Event){			y -= vitesseUp;			if(y<490){				dispatchEvent(new Event(CHOPPE,true));			}		}				private function descend(e:Event){			addEventListener(Event.ENTER_FRAME, frameRateDown);		}				private function frameRateDown(e:Event){			y -= vitesseUp;			if(y<490){				dispatchEvent(new Event(CHOPPE,true));			}		}			}// End of class}