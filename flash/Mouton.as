package  {		import flash.display.MovieClip;	import flash.utils.*;	import flash.events.*;		public class Mouton extends MovieClip {				public var numero:Number;		private const vitesseUp:Number = 5;		public static const CHOPPE:String = "moutonChoppe";				public function Mouton() {			y = 530 + Math.random()*50;			x = -120;		}				public function monte(){			addEventListener(Event.ENTER_FRAME, frameRateUp);		}				private function frameRateUp(e:Event){			y -= vitesseUp;			if(y<500){				dispatchEvent(new Event(CHOPPE,true));			}		}	}	}