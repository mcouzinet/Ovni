package  {	import flash.display.MovieClip;	import flash.events.*;	import fl.transitions.Tween;	import fl.transitions.easing.*;		public class Soucoupe extends MovieClip {				public var monte:Boolean = false;		private var vitesse:uint = 4;				public function Soucoupe(posY,posX) {			x = posX;			y = posY;		}				public function bouge(pos){			var deplacement:Tween = new Tween(this, "y", Strong.easeOut, this.y, (500-pos/2), 0.5, true);		}			} //end of class}