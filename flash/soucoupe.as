package  {	import flash.display.MovieClip;	import flash.events.*;		public class Soucoupe extends MovieClip {				public var monte:Boolean = false;		private var vitesse:uint = 4;				public function Soucoupe(posY,posX) {			x = posX;			y = posY;			addEventListener(Event.ENTER_FRAME, frameRate);		}				private function frameRate(e:Event){			if( monte ){					if( y > 0 ){					y -= vitesse;				}			}else{				if( y < 500 ){					y += vitesse;				} 			}		}			} //end of class}